{
  "language": "ts",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/portal/src",
  "compilerOptions": {
    "plugins": ["@nestjs/graphql"],
    "tsConfigPath": "apps/portal/tsconfig.app.json",
    "webpackConfigPath": "webpack.config.js",
    "webpack": true,
    "deleteOutDir": true
  },
  "monorepo": true,
  "root": "apps/portal",
  "projects": {
    "portal": {
      "type": "application",
      "root": "apps/portal",
      "entryFile": "main",
      "sourceRoot": "apps/portal/src",
      "compilerOptions": {
        "tsConfigPath": "apps/portal/tsconfig.app.json"
      }
    },
    "sync": {
      "type": "application",
      "root": "apps/sync",
      "entryFile": "main",
      "sourceRoot": "apps/sync/src",
      "compilerOptions": {
        "tsConfigPath": "apps/sync/tsconfig.app.json"
      }
    },
    "sync-job": {
      "type": "application",
      "root": "apps/sync-job",
      "entryFile": "main",
      "sourceRoot": "apps/sync-job/src",
      "compilerOptions": {
        "tsConfigPath": "apps/sync-job/tsconfig.app.json"
      }
    },
    "soap": {
      "type": "library",
      "root": "libs/soap",
      "entryFile": "index",
      "sourceRoot": "libs/soap/src",
      "compilerOptions": {
        "tsConfigPath": "libs/soap/tsconfig.lib.json"
      }
    },
    "config": {
      "type": "library",
      "root": "libs/config",
      "entryFile": "index",
      "sourceRoot": "libs/config/src",
      "compilerOptions": {
        "tsConfigPath": "libs/config/tsconfig.lib.json"
      }
    },
    "image": {
      "type": "library",
      "root": "libs/image",
      "entryFile": "index",
      "sourceRoot": "libs/image/src",
      "compilerOptions": {
        "tsConfigPath": "libs/image/tsconfig.lib.json"
      }
    },
    "logging.interceptor": {
      "type": "library",
      "root": "libs/logging.interceptor",
      "entryFile": "index",
      "sourceRoot": "libs/logging.interceptor/src",
      "compilerOptions": {
        "tsConfigPath": "libs/logging.interceptor/tsconfig.lib.json"
      }
    },
    "cache.interceptor": {
      "type": "library",
      "root": "libs/cache.interceptor",
      "entryFile": "index",
      "sourceRoot": "libs/cache.interceptor/src",
      "compilerOptions": {
        "tsConfigPath": "libs/cache.interceptor/tsconfig.lib.json"
      }
    }
  }
}
